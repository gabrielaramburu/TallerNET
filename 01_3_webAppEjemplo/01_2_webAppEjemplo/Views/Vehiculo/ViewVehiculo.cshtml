@{
    //establece la propiedad Title, la cual puede ser usada(leida) desde el template
    ViewData["Title"] = "Ejemplo básico de transferencia de datos desde controlador a vista usando ViewModel";
}

<h2>Index</h2>

<p>Lista generada en base a parámetros del request </p>

<ul>
    @for (int i = 0; i < (int)ViewData["Value"]!; i++)
    {
        <li>@ViewData["Name"]</li>
    }
</ul>

